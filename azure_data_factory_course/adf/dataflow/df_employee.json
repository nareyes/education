{
	"name": "df_employee",
	"properties": {
		"type": "MappingDataFlow",
		"typeProperties": {
			"sources": [
				{
					"dataset": {
						"referenceName": "ds_blob_input_employee",
						"type": "DatasetReference"
					},
					"name": "blobEmployee"
				}
			],
			"sinks": [
				{
					"dataset": {
						"referenceName": "ds_adls_output_folder",
						"type": "DatasetReference"
					},
					"name": "adlsEmployee"
				}
			],
			"transformations": [],
			"scriptLines": [
				"source(output(",
				"          Emp_id as integer,",
				"          Name as string,",
				"          Gender as string,",
				"          Salary as integer,",
				"          Dept_id as integer",
				"     ),",
				"     allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     inferDriftedColumnTypes: true,",
				"     ignoreNoFilesFound: true,",
				"     moveFiles: ['','processed/'],",
				"     partitionBy('hash', 1)) ~> blobEmployee",
				"blobEmployee sink(allowSchemaDrift: true,",
				"     validateSchema: false,",
				"     partitionFileNames:['employee.csv'],",
				"     umask: 0022,",
				"     preCommands: [],",
				"     postCommands: [],",
				"     skipDuplicateMapInputs: true,",
				"     skipDuplicateMapOutputs: true,",
				"     mapColumn(",
				"          Emp_Id = Emp_id,",
				"          Name,",
				"          Gender,",
				"          Salary,",
				"          Dept_Id = Dept_id",
				"     ),",
				"     partitionBy('hash', 1)) ~> adlsEmployee"
			]
		}
	}
}